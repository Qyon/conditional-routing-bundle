{
    "name": "piotrpolak/conditional-routing-bundle",
    "description": "Provides a way to selectively load Symfony bundle routes based on a set of user defined conditions.",
    "type": "symfony-bundle",
    "require": {
        "php": ">=5.3.0",
        "symfony/routing": "~2.3|~3.1",
        "symfony/dependency-injection": "~2.3|~3.1",
        "symfony/config": "~2.3|~3.1",
        "symfony/http-kernel": "~2.3|~3.1",
        "symfony/framework-bundle": "~2.3|~3.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Piotrpolak\\ConditionalRoutingBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Piotr Polak",
            "email": "piotr@polak.ro"
        }
    ],
    "require-dev": {
        "symfony/expression-language": "~2.3|~3.0",
        "codeclimate/php-test-reporter": "^0.3.2",
        "phpmd/phpmd": "^2.6"
    }
}
